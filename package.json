    "lint:fix": "yarn prettier:fix && yarn eslint:fix",
    "prettier": "prettier --check '{src,__tests__}/**/*.ts'",
    "prettier:fix": "prettier --write '{src,__tests__}/**/*.ts'",
    "eslint": "eslint {src,__tests__}/**/*.ts",
    "eslint:fix": "eslint --fix {src,__tests__}/**/*.ts",
