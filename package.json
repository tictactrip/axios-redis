{
  "name": "@tictactrip/axios-redis",
  "version": "1.5.1",
  "description": "Axios Redis cache middleware",
  "author": "Tictactrip <dev@tictactrip.eu>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "axios": "0.21.0",
    "flatted": "3.1.0",
    "lodash": "4.17.20",
    "redis": "3.0.2"
  },
  "devDependencies": {
    "@types/jest": "26.0.15",
    "@types/node": "14.14.7",
    "@types/redis": "2.8.28",
    "jest": "26.6.3",
    "nock": "13.0.5",
    "prettier": "2.1.2",
    "ts-jest": "26.4.4",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-microsoft-contrib": "6.2.0",
    "typescript": "4.0.5"
  },
  "scripts": {
    "clean": "rm -rf coverage dist tmp",
    "build": "yarn clean && tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "prettier": "prettier --check '{src,__tests__}/**/*.ts'",
    "prettier:fix": "prettier --write '{src,__tests__}/**/*.ts'",
    "tslint": "tslint -t stylish --project 'tsconfig.json'",
    "tslint:fix": "tslint --fix -t stylish --project 'tsconfig.json'",
    "lint": "yarn tslint && yarn prettier",
    "lint:fix": "yarn tslint:fix || true && yarn prettier:fix",
    "test": "docker-compose up --abort-on-container-exit"
  }
}
